{
  "name": "camille-davis/local-wordpress",
  "description": "Local WordPress site with XDebug, docker, and linting",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.10",
